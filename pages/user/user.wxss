image{width: 100%;height: 100%;}
page{
  padding: 15px;
  box-sizing: border-box;
}

.System_bar{
  width: 100%;
  box-sizing: border-box;
}

.head{
  width: 100%;
  height: 180rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 50rpx;
}

.head_img{
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  background-size: cover;
  overflow: hidden;
}

.logo {
  width: 128rpx;
  height: 128rpx;
  margin: 0rpx auto;
  position: relative;
  top: 200rpx;
}

.logoImg {
  width: 100%;
  height: 100%;
}

.loginButton {
  width: 100%;
  margin: 0rpx auto;
  position: relative;
  top: 500rpx;
}

.wechatLogin, .phoneLogin {
  height: 80rpx;
  border-radius: 50rpx;
  border: 0rpx;
  outline: none;
}
.wechatLogin, .phoneLogin .icon-message{
  width: 100%;
}
.wechatLogin button {
  width: 70%;
  background-color: #70e500;
  border-radius: 50rpx;
  border: none;
  color: #fff;
}

.phoneLogin {
  margin-top: 40rpx;
}

.phoneLogin button {
  border-radius: 50rpx;
  border: 1rpx solid #70e500;
  background-color: #fff;
  color: #70e500;
}

.tab{
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom:2PX solid #F5F5F5;
  font-size: 28rpx;
  font-weight: 600;
}
.tab image{
  width: 42rpx;
  height: 42rpx;
}