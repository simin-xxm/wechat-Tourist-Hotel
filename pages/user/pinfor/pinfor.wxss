

page{
  padding: 15px;
  box-sizing: border-box;
}
image{
  width: 100%;
  height: 100%;
}
.System_bar{
  width: 100%;
  box-sizing: border-box; 
}
.System_bar image{
  width: 45rpx;
  height: 45rpx;
}
.txt{
  font-size: 45rpx;
  font-weight: bold;
}
.head{
  margin-top: 50rpx;
  width: 100%;
  height: 250rpx;
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
}
.head .tou{
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  overflow: hidden;
  background-image: url('https://ss3.bdstatic.com/70cFv8Sh_Q1YnxGkpoWK1HF6hhy/it/u=15069710,4207879693&fm=26&gp=0.jpg');
  background-size: cover;
}
.head .icon{
  position: absolute;
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
  top: 64%;
  left: 57%;
  overflow: hidden;
  background-color: #13C2C2;
  display: flex;
  justify-content: center;
  align-items: center;
}
.head .icon image{
  width: 75%;
  height: 75%;
}
.tab{
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom:2PX solid #F5F5F5;
  font-size: 26rpx;
  font-weight: 600;
}
.tab_s{
  font-size: 26rpx;
  color: #ccc;
}
